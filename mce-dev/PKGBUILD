# Maintainer: Deepak Meena <notwho53@gmail.com>
pkgname=mce-dev
pkgver=1.32.0
pkgrel=1
pkgdesc="development files for mce"
arch=('aarch64')
url="https://github.com/sailfishos/mce-dev.git"
license=('custom')
source=("git+${url}")
sha256sums=('SKIP')

build() {
	cd $srcdir/$pkgname
	make KEEP_SYMBOLS=1 build
}

package() {
	cd $srcdir/$pkgname
	make DESTDIR="${pkgdir}" install
}
